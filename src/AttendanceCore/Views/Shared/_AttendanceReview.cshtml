@using System.Threading.Tasks
@using AttendanceCore.Services
@inject IEntryService Service

<table class="table table-striped">
    <thead>
    <tr>
        <th>Username</th>
        <th>Type</th>
        <th>Time</th>
        <th>Note</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in await Service.GetPageAsync(0))
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Type</td>
            <td>@item.Time</td>
            <td>@item.Note</td>
        </tr>
    }
    </tbody>
</table>